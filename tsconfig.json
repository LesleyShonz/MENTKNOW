{
    "include": [
        "src/**/*"
    ],
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "target": "esnext",
        "jsx": "react-jsx","moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
       
        "noImplicitAny": false
    }
}